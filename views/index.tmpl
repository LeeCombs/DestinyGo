<html>
    <h1>
        {{.title}}
    </h1>
    <h2>
        {{.displayName}}
    </h2>
    <h3>
        {{ range .a }}
        <p>{{ . }}</p>
        {{ end }}
    </h3>
    <h3>
        {{ range $k, $v := .chars }}
        <p><strong>{{ $k }}</strong>:{{ $v }}</p>
        {{ end }}
    </h3>
    <body>
        <form action="/searchUser" method="post">
            displayName:<input type="text" name="displayName">
            <input type="submit" name="search">
        </form>
    </body>
</html>
